    "yaml": "1.7.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "BROWSER=none REACT_APP_KUBEAPPS_NS=${TELEPRESENCE_CONTAINER_NAMESPACE} react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "yarn run build-css && react-scripts test",
    "eject": "react-scripts eject",
    "tslint-check": "tslint-config-prettier-check ./tslint.ci.json",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,scss}'",
    "prettier-check": "prettier --check 'src/**/*.{ts,tsx,scss}'",
    "lint-js": "tslint -c ./tslint.ci.json 'src/**/*.{ts,tsx}'",
    "lint-js-fix": "tslint -c ./tslint.ci.json --fix 'src/**/*.{ts,tsx}'",
    "lint-css": "stylelint --config ./.stylelintrc.json 'src/**/*.scss'",
    "lint-css-fix": "stylelint --config ./.stylelintrc.json --fix 'src/**/*.scss'",
    "lint": "npm-run-all lint-js lint-css",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run lint-js-fix",
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "yarn run lint-css-fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react-dom": "^16.9.5",
    "@types/react-modal": "^3.1.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.5",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-mock-store": "^1.0.0",
    "enzyme": "^3.11.0",
    "husky": "^1.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^10.2.11",
    "mock-socket": "^8.0.2",
    "moxios": "^0.4.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.10.2",
    "react-scripts": "3.4.1",
    "redux-mock-store": "^1.5.3",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-react": "^3.4.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*",
      "!src/**/*.d.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@clr|lit-element|lit-html|ramda|.*css)"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run ts-compile-check && lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
