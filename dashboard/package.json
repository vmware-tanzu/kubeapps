    "@types/json-schema": "^7.0.3",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.117",
    "@types/moxios": "^0.4.8",
    "@types/pako": "^1.0.0",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.9.41",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-jsonschema-form": "^1.0.4",
    "@types/react-router-dom": "^5.1.5",
