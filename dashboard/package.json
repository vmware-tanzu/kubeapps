{
  "name": "dashboard",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "build": "npm-run-all build-css compile-lang build-js",
    "build-css": "sass src/ --no-source-map && yarn run copy-clr",
    "build-js": "react-scripts build",
    "compile-lang": "formatjs compile-folder lang src/locales/ --ast --format simple",
    "copy-clr": "shx cp ./node_modules/@clr/ui/clr-ui-dark.min.css public/clr-ui-dark.min.css && shx cp node_modules/@clr/ui/clr-ui.min.css public/clr-ui.min.css && shx cp ./node_modules/@clr/ui/clr-ui-dark.min.css.map public/clr-ui-dark.min.css.map && shx cp ./node_modules/@clr/ui/clr-ui.min.css.map public/clr-ui.min.css.map",
    "eject": "react-scripts eject",
    "eslint-check": "eslint --config ./.eslintrc.json 'src/**/*.{js,ts,tsx}' --max-warnings=0",
    "eslint": "eslint --config ./.eslintrc.json 'src/**/*.{js,ts,tsx}' --fix --max-warnings=0",
    "extract-lang": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "lint": "npm-run-all lint-css-check eslint-check",
    "lint-css-check": "stylelint --config ./.stylelintrc.json 'src/**/*.scss'",
    "lint-css": "stylelint --config ./.stylelintrc.json --fix 'src/**/*.scss'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,scss}'",
    "prettier-check": "prettier --check 'src/**/*.{ts,tsx,scss}'",
    "start": "npm-run-all -p watch-css compile-lang start-js",
    "start-js": "BROWSER=none react-scripts start",
    "test": "yarn run build-css && yarn run compile-lang && react-scripts test",
    "ts-compile-check": "yarn run compile-lang && tsc -p tsconfig.json --noEmit",
    "watch-css": "yarn run build-css && yarn run compile-lang && sass src/ --watch --no-source-map"
  },
  "dependencies": {
    "@cds/city": "^1.1.0",
    "@cds/core": "^5.6.2",
    "@cds/react": "^5.6.2",
    "@clr/ui": "^13.0.1",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@paciolan/remote-component": "^2.12.1",
    "ajv": "^8.10.0",
    "axios": "^0.26.0",
    "connected-react-router": "^6.9.2",
    "fast-json-patch": "^3.1.0",
    "google-protobuf": "^3.19.4",
    "history": "^4.10.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "prop-types": "^15.8.1",
    "protobufjs": "^6.11.2",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-ace": "^9.5.0",
    "react-compound-slider": "^3.3.1",
    "react-copy-to-clipboard": "^5.0.4",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.24.6",
    "react-markdown": "^8.0.0",
    "react-minimal-pie-chart": "^8.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-router-hash-link": "^2.4.3",
    "react-switch": "^6.0.0",
    "react-tabs": "^3.2.3",
    "react-test-renderer": "^17.0.2",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "remark-gfm": "^3.0.1",
    "rxjs": "^7.5.4",
    "swagger-ui-react": "^4.5.0",
    "typesafe-actions": "^5.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.2",
    "@improbable-eng/grpc-web-fake-transport": "^0.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/moxios": "^0.4.14",
    "@types/qs": "^6.9.7",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/react-transition-group": "^4.4.4",
    "@types/redux-mock-store": "^1.0.3",
    "@types/swagger-ui-react": "^4.1.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-redos": "^4.3.0",
    "jest-enzyme": "^7.1.2",
    "jest-plugin-context": "^2.9.0",
    "mock-socket": "^9.1.2",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "react-scripts": "^4.0.3",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.49.7",
    "shx": "^0.3.4",
    "stylelint": "^14.5.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.1.3",
    "ts-proto": "^1.104.0",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@babel/parser": "^7.15.3"
  },
  "jest": {
    "resetMocks": false,
    "collectCoverageFrom": [
      "src/**/*",
      "!src/**/*.d.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@cds|@clr|@lit|bail|ccount|character-entities|comma-separated-tokens|decode-named-character-reference|escape-string-regexp|hast-util-whitespace|is-plain-obj|lit|lodash-es|markdown-table|mdast-util-definitions|mdast-util-find-and-replace|mdast-util-from-markdown|mdast-util-gfm-autolink-literal|mdast-util-gfm|mdast-util-to-hast|mdast-util-to-markdown|mdast-util-to-string|micromark-core-commonmark|micromark|parse-entities|property-information|ramda|react-markdown|react-markdown|remark-gfm|remark-parse|remark-rehype|space-separated-tokens|trough|unified|unist-builder|unist-util-generated|unist-util-is|unist-util-position|unist-util-stringify-position|unist-util-visit-parents|unist-util-visit|util-find-and-replace|vfile-message|vfile|.*css)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
